{
  "version": "7.0.0-rc1-final-l01",
  "description": "SQLite data store for Entity Framework.",
  "repository": {
    "type": "git",
    "url": "git://github.com/aspnet/entityframework"
  },
  "compilationOptions": {
    "warningsAsErrors": true,
    "keyFile": "../../tools/Key.snk"
  },
  "dependencies": {
    "EntityFramework.Relational": "7.0.0-rc1-final-l01",
    "Microsoft.Data.Sqlite": "1.0.0-rc1-final"
  },
  "compile": "..\\Shared\\*.cs",
  "namedResource": {
    "EntityFramework.Sqlite.SqliteStrings": "Properties/SqliteStrings.resx"
  },
  "resource": [
    "Properties/EntityFramework.Sqlite.rd.xml"
  ],
  "frameworks": {
    "net451": { },
    "dotnet5.4": {
      "dependencies": {
        "System.IO.FileSystem": "4.0.1-beta-23516",
        "Microsoft.CSharp": "4.0.1-beta-23516"
      }
    },
    "netcore50": {
      "dependencies": {
        "System.IO.FileSystem": "4.0.1-beta-23516"
      }
    }
  }
}